


<div class="home-head d-flex justify-content-center">
  <div class="text-center dog_card">
    <p class="title">Share Your Dog.</p>
      <div class="custom-search">
        <input type="text" class="custom-search-input" placeholder="Search a dog to rent...">
        <button class="custom-search-botton" type="submit"><i class="fa-solid fa-dog"></i></button>
    </div>
  </div>
</div>





<div class="row">
  <% @dogs.each do |dog| %>
  <div class="col-3">
    <%= render 'pages/card', dog: dog %>
  </div>
  <% end %>
</div>
